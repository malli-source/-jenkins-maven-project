pipeline { 
    agent any
    stages {
        
        stage('Compile') {
            steps {
            java --version
            bat  "mvn compile"
            }
        }
        
        stage('Test') {
            steps {
                bat "mvn test"
            }
        }
        
        stage('Package') {
            steps {
                bat "mvn package"
            }
        }
    }
}
