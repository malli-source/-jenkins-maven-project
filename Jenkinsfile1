pipeline { 
    agent any
    stages {
        stage('Test') {
            steps {
                bat "mvn test -f first-app/first-app/pom.xml"
            }
        }
        
        stage('Package') {
            steps {
                bat "mvn package -f first-app/first-app"
            }
        }

        stage('Package') {
            steps {
              script {
                withSonarQubeEnv(credentialsId: 'sonar') {
                  sh 'mvn -f first-app/first-ap/pom.xml clean package sonar:sonar -Dsonar.projectKey=malli-sonar -Dsonar.projectName=malli-sonar'
                }
              }
            }
        }      
    }
}
