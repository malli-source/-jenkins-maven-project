pipeline { 
    agent any
    stages {
        
        stage('Compile') {
            steps {
            bat "java --version"
            bat  "mvn compile"
            }
        }
        
        stage('Test') {
            steps {
                bat "mvn -f first-app/first-app/pom.xml"
            }
        }
        
        stage('Package') {
            steps {
                bat "mvn package"
            }
        }
    }
}
